{
  "name": "@kb-labs/shared-command-kit",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "description": "Command Kit for KB Labs CLI - utilities and high-level API for building commands",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./flags": {
      "import": "./dist/flags/index.js",
      "types": "./dist/flags/index.d.ts"
    },
    "./analytics": {
      "import": "./dist/analytics/index.js",
      "types": "./dist/analytics/index.d.ts"
    },
    "./errors": {
      "import": "./dist/errors/index.js",
      "types": "./dist/errors/index.d.ts"
    },
    "./helpers": {
      "import": "./dist/helpers/index.js",
      "types": "./dist/helpers/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsup --config tsup.config.ts",
    "dev": "tsup --config tsup.config.ts --watch",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@kb-labs/analytics-sdk-node": "link:../../../kb-labs-analytics/packages/analytics-sdk-node",
    "@kb-labs/cli-contracts": "link:../../../kb-labs-cli/packages/cli-contracts",
    "@kb-labs/shared-cli-ui": "workspace:*"
  },
  "devDependencies": {
    "@kb-labs/devkit": "link:../../../kb-labs-devkit",
    "@types/node": "^24.3.3",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.6.3",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.11.0"
}
