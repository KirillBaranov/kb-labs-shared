{
  "name": "@kb-labs/perm-presets",
  "version": "0.1.1",
  "description": "Composable permission presets for KB Labs plugins",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "dev": "tsup --config tsup.config.ts --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {},
  "devDependencies": {
    "@kb-labs/devkit": "link:../../../kb-labs-devkit",
    "typescript": "^5.6.3",
    "tsup": "^8.5.0",
    "rimraf": "^6.0.1",
    "vitest": "^3.2.4",
    "@types/node": "^24.3.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "private": true,
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  }
}
